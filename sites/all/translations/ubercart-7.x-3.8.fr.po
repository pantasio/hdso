msgstr "Titulaire de la carte :"
msgid "Card Number:"
msgstr "Numéro de la carte :"
msgid "Start Date:"
msgstr "Date de début :"
msgid "Expiration:"
msgstr "Date d'expiration :"
msgid "Issue Number:"
msgstr "Numéro de publication :"
